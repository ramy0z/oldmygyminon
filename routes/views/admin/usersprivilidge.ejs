<div id="main">
				<!-- EDIT Form -->
				<div class="add-form-edit">
					<form id="editform" method="post" action="">
						<h2 class="form-heading">Edit User Privilidge</h2>
						<div class="form-group">
						<label for="">Privilidge Type</label><br >
						<select id="updatecheckboxid" name="type" onchange="updatecheckbox()" >
								<option value="5c5ee0444e44ee1054e26a89">No</option>
							<% Object.keys(privilodgeType).forEach(function(key) { %>
										<option id="<%- privilodgeType[key].type %>" value="<%- privilodgeType[key]._id %>"><%- privilodgeType[key].type %></option>
							<% });	 %>
							</select>
					</div>

					<% Object.keys(privilodgeType).forEach(function(key) { %>
						<input type="hidden" id="<%- privilodgeType[key]._id %>" value="<%- privilodgeType[key].privilidge %>">
						<% });	 %>

							<div class="form-group">
							<label for="">All privilidge</label><br >
								<ul>
								<% var i=1;
								Object.keys(allPrivilidge).forEach(function(key1) { %>
									<li style="margin:10px;">
										<input type="checkbox" id="selectall<%- i %>" onclick="selectall('selectall<%- i %>')" class="allPrivilidgecheck" /> <%- key1 %> <ul style="margin-left: 10px;">
									<% Object.keys(allPrivilidge[key1]).forEach(function(key) { %>
										<li><input type="checkbox" onclick="selectone('selectall<%- i %>')" class="allPrivilidgecheck selectall<%- i %>" name="privilidge" id="<%- allPrivilidge[key1][key] %>" value="<%- allPrivilidge[key1][key] %>" />
										<span><%- key %></span></ li>
									<% }); %>
									</ul></li>
								<% i++; }); %>
								</ul>
						</div>
						<div class="form-group">
							<input type="submit" class="form-control" >
							<input type="button" class="form-control cancel-edit" value="Cancel">

						</div>
					</form>
				</div>
				<!-- End Edit Form -->

				<ol class="breadcrumb">
						<li><a href="/admin">Home</a></li>
						<li class="active">Users Privilidge</li>
				</ol>
				<!-- //breadcrumb-->

				<div id="content">

						<div class="row">

								<div class="col-lg-12">

										<section class="panel">
												<header class="panel-heading">
														<h2><strong>Users </strong> Privilidge </h2>
												</header>
												<div class="panel-tools fully color" align="right"  data-toolscolor="#6CC3A0">
														<ul class="tooltip-area">
																<li><a href="javascript:void(0)" class="btn btn-collapse" title="Collapse"><i class="fa fa-sort-amount-asc"></i></a></li>
																<li><a href="javascript:void(0)" class="btn btn-reload"  title="Reload"><i class="fa fa-retweet"></i></a></li>
																<li><a href="javascript:void(0)" class="btn btn-close" title="Close"><i class="fa fa-times"></i></a></li>
														</ul>
												</div>
												<div class="panel-body">
																<div class="table-responsive">
																<table cellpadding="0" cellspacing="0" border="0" class="table table-bordered table-striped">
																		<thead>
																				<tr>
																								<th>No.</th>
																								<th>Name</th>
																								<th>type</th>


																								<th width="30%">Action</th>
																				</tr>
																		</thead>
																		<tbody align="center">
																					<%
																					var i=1;
																					for (var key in usersPrivilidge) {
																						if (usersPrivilidge.hasOwnProperty(key)) {

																							if(! Object.keys(usersPrivilidge[key].users).length) continue;
																							//remove club admin
																							//if(usersPrivilidge[key].key == session.parent_key) continue;
																							//get typeo
																							if(typeof usersPrivilidge[key].privilidgetypes[0] == 'undefined') var type='no';
																							else var type = usersPrivilidge[key].privilidgetypes[0].type;																						 %>
																						 <tr>
																							<td><%- i %></td>
																							<td> <%- usersPrivilidge[key].users[0].name  %> </td>
																							<td> <%- type  %> </td>
																							<!-- <td> <%- usersPrivilidge[key].privilidge  %> </td> -->
																							<td>
																								<span class="tooltip-area">
																								<a href="javascript:void(0)" onclick="editprivilidge('<%- usersPrivilidge[key].privilidge  %>','<%- usersPrivilidge[key]._id  %>','<%- type  %>')" class="btn btn-default btn-sm" title="Edit"><i class="fa fa-pencil"></i></a>
																								<a href="/admin/usersprivilidge/<%- usersPrivilidge[key]._id %>"  class="btn btn-default btn-sm" title="Delete"><i class="fa fa-trash-o"></i></a>
																								</span>
																							</td>
																						 </tr>
																					<%
																							}
																							i++;
																						}
																					%>


																		</tbody>
																</table>
																</div>
												</div>
										</section>
								</div>

						</div>
						<!-- //content > row-->

				</div>
				<!-- //content-->


		</div>
